<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exam.customui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.exam.app.Myapp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CurrentPreviousActivity"
            android:label="@string/title_activity_current_previous" >
        </activity>
        <activity
            android:name=".MeetActivity"
            android:label="@string/title_activity_meet" >
        </activity>
        <activity
            android:name=".UserMessageActivity"
            android:label="@string/title_activity_user_message" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".VoteExplainActivity"
            android:label="@string/title_activity_vote_explain" >
        </activity>
        <activity
            android:name=".VoteSelectActivityActivity"
            android:label="@string/title_activity_vote_select_activity" >
        </activity>
        <activity
            android:name=".VoteSelectActivity"
            android:label="@string/title_activity_vote_select" >
        </activity>
        <activity
            android:name=".CandidateActivity"
            android:label="@string/title_activity_candidate" >
        </activity>
    </application>

</manifest>
